vogue-flex-start-row() {
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
}

vogue-flex-center-row() {
  display: flex;
  flex-direction: row;
  justify-content: center;
}

vogue-flex-end-row() {
  display: flex;
  flex-direction: row;
  justify-content: flex-end;
}

vogue-flex-space-row() {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
