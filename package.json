{
  "name": "recomp-js",
  "private": true,
  "version": "0.0.0",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "yarn workspace build",
    "storybook": "yarn workspace @recomp/storybook storybook",
    "storybook-olp": "SET NODE_OPTIONS=--openssl-legacy-provider && yarn run storybook",
    "build-storybook": "yarn workspace @recomp/storybook build-storybook"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.6.7",
    "@storybook/addon-interactions": "^7.6.7",
    "@storybook/addon-links": "^7.6.7",
    "@storybook/addon-onboarding": "^1.0.10",
    "@storybook/blocks": "^7.6.7",
    "@storybook/react": "^7.6.7",
    "@storybook/react-vite": "^7.6.7",
    "@storybook/test": "^7.6.7",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "less": "^4.1.3",
    "sass": "^1.62.1",
    "storybook": "^7.6.7",
    "storybook-dark-mode": "^3.0.3",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  },
  "prettier": {
    "trailingComma": "es5",
    "singleQuote": true,
    "arrowParens": "always",
    "endOfLine": "auto"
  },
  "packageManager": "yarn@1.22.1"
}
